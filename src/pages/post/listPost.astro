---
import BaseHead from "../../components/BaseHead.astro";
import CardPost from "../../components/cardPost.astro";
import { SITE_DESCRIPTION, SITE_TITLE } from "../../consts";
import Header from "../../components/Header.astro";
import Footer from "../../components/Footer.astro";
const imgPlaceholder = "../../assets/blog-placeholder.png";

const posts: Post[] = [
  {
    id:1,
    description: "Description 1",
    userId: 1,
    img: "data:image/png;base64,",
    tittle: "Post 1",
    ingredients: [{name:"Ingredients 1",quantity:"500 gramos"}],
  },
  {
    id:2,
    description: "Description 1",
    userId: 1,
    img: "data:image/png;base64,",
    tittle: "Post 1",
    ingredients: [{name:"Ingredients 1",quantity:'300ml'}],
  },
  {
    id:4,
    description: "Description 1",
    userId: 1,
    img: "data:image/png;base64,",
    tittle: "Post 1",
    ingredients: [{name:"Ingredients 1",quantity:'400ml'}],
  },
];

function editPost(id: number) {
  console.log(id);
}
---

<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <body class="flex flex-col min-h-screen">
    <Header />
    <main class="flex-1 p-2 gap-2 flex flex-wrap justify-start items-center">
      {
        posts.map((post: Post) => {
          return <CardPost post={post} edit={() => editPost(post.id)} />;
        })
      }
    </main>
    <Footer />
  </body>
</html>
